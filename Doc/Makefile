PM_DIR := ..
include $(PM_DIR)/Make/Makefile.config

###-----------------------------------------------------------------------------
### Targets
###-----------------------------------------------------------------------------
TARGETS = Tutorial.html TODO.html

Tutorial.html: ../Test/tutorial.C

.PHONY: all
all: $(TARGETS)

.PHONY: clean distclean
clean distclean:
	rm -f $(TARGETS)

